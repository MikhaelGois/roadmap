{
  "name": "entrega-roteirizada-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --transpile-only --respawn src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "seed": "ts-node src/seed.ts",
    "test": "jest --runInBand",
    "test:routes": "jest src/routes/__tests__/assign.test.ts --runInBand",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "aws-sdk": "^2.1354.0",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "minio": "^7.0.23",
    "pg": "^8.11.0",
    "socket.io": "^4.8.0",
    "dotenv": "^16.0.0",
    "multer": "^1.4.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.2",
    "prisma": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.5.0",
    "supertest": "^6.4.0",
    "@types/supertest": "^2.0.12"
  }
}